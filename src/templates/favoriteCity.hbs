<div class="favotite-city__item swiper-slide" name="{{city}}">
    <span class="favorite-city__name" data-city="openCity">{{city}}</span>
    <button class="close" data-close="closeMe">
        <img data-close="closeMe"
            src="data:image/png;base64,"
            alt="" name="{{city}}">
    </button>

</div>